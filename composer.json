{
    "name": "victoryoalli/laravel-code-generator",
    "description": "A PHP Package for Code Generation.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Yoalli",
            "email": "victoryoalli@gmail.com"
        }
    ],
    "require": {
        "illuminate/console": "^5.5",
        "illuminate/filesystem": "^5.5",
        "doctrine/dbal": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "VictorYoalli\\LaravelCodeGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VictorYoalli\\LaravelCodeGenerator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VictorYoalli\\LaravelCodeGenerator\\LaravelCodeGeneratorServiceProvider"
            ],
            "aliases": {
                "LaravelCodeGenerator": "VictorYoalli\\LaravelCodeGenerator\\LaravelCodeGenerator",
                "LCG": "VictorYoalli\\LaravelCodeGenerator\\Helper"
            }
        }
    }
}